rule"Controllo #5_CHF"
    dialect"mvel"
    when
        
        dayCounter: DayCounter()
        log: Logger()


         fx_ASK:  CurveStructure(
            fl_input =="I",
            cod_asset =="FX-SPOT-CHF",
            cod_type =="ASK")   
         fx_BID:  CurveStructure(
            fl_input =="I",
            cod_asset =="FX-SPOT-CHF",
            cod_type =="BID")         
         spiti_ASK:  CurveStructure(
            fl_input ==	"I",
            cod_asset =="SPITI-CHF",
            cod_type =="ASK")    
         spiti_BID:  CurveStructure(
            fl_input =="I",
            cod_asset =="SPITI-CHF",
            cod_type =="BID")   
         dyn_soglia_ask:  CurveStructure(
            fl_input =="I",
            cod_asset =="DYN-SPITI-CHF",
            cod_type =="ASK") 
