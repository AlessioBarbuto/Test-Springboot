rule"Controllo #11_GBP"
    dialect"mvel"
    when
        
        dayCounter: DayCounter()
        log: Logger()


         fx_ASK:  CurveStructure(
            fl_input =="I",
            cod_asset =="FX-SPOT-GBP",
            cod_type =="ASK")   
         fx_BID:  CurveStructure(
            fl_input =="I",
            cod_asset =="FX-SPOT-GBP",
            cod_type =="BID")       
         spiti_ASK:  CurveStructure(
            fl_input ==	"I",
            cod_asset =="SPITI-GBP",
            cod_type =="ASK")    
         spiti_BID:  CurveStructure(
            fl_input =="I",
            cod_asset =="SPITI-GBP",
            cod_type =="BID")    
         dyn_soglia_ask:  CurveStructure(
            fl_input =="I",
            cod_asset =="DYN-SPITI-GBP",
            cod_type =="ASK")
         dyn_soglia_bid:  CurveStructure(
            fl_input =="I",
            cod_asset =="DYN-SPITI-GBP",
            cod_type =="BID")           
    then
    
      OUT_list list = new OUT_list();
      String tenor="";
      Double irsMid = 0;
      Double spread = null;
 	  Data d = null;
 	  Double fx = 0;
 	  Double year_frac = 0;
 	  Double basisEUR=36000;
 	  Double basisCCY=36500;
 	  Double days = 0;
 	  Double spiti3_USD_ASK = 0;
 	  Double spiti3_USD_BID = 0;
 	  
 	  log.debug("Controllo #11 GBP")
	  log.debug("TENOR\tTYPE\tDTMATURITY\tFX SPOT MID\tFX MID\tSPITI3\tDAYS\tSPREAD")
      
      
      Double fx_SPOT_MID = (fx_ASK.getBucket("Spot")+fx_BID.getBucket("Spot"))/2d;
      Data dtRif = spiti_ASK.getAsOfDate();

      tenor = "ON"
      spiti_USD_ASK =  (Double)myMap.get(tenor+"_"+spiti_ASK.getCod_type());
      spiti_USD_BID =   (Double)myMap.get(tenor+"_"+spiti_BID.getCod_type());

      fx = (fx_ASK.getBucket(tenor)+fx_BID.getBucket(tenor))/(2d * 10000d);
